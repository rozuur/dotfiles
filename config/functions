# Place file name in header when catting multiple files
function mcat()
{
    more "$@" | cat
}

# Clear screen, cygwin seems to not have clear command
function clr()
{
    if [ command -v clear 2>/dev/null ]; then
        clear
    else
        printf "\033c"
    fi
}

function ff() {
    find . -type f -iname "$@*" ;
}
function ffe() {
    find . -type f -iname "*$@" ;
}

function ip() {
  curl icanhazip.com;
}
